{
	"compilerOptions": {
		"declaration": true,
		"inlineSources": true,
		"lib": [
			"dom",
			"es5",
			"es2015.iterable",
			"es2015.promise",
			"es2015.symbol",
			"es2015.symbol.wellknown"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "_build/",
		"removeComments": false,
		"rootDir": ".",
		"sourceMap": true,
		"strict": true,
		"target": "es5",
		"types": [ "intern" ]
	},
	"include": [
		"./src/**/*.ts"
	]
}
